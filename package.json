{
  "name": "ryoumengo-server",
  "version": "1.0.0",
  "description": "Anki node server",
  "main": "dist/app.js",
  "scripts": {
    "start": "tsoa spec-and-routes && tsc && node dist/src/app.js",
    "start:dev": "tsoa spec-and-routes && nodemon -L --watch src/ -e ts index.js",
    "lint": "eslint --ext .ts src/**/*.ts",
    "compile": "tsc --noEmit",
    "doc": "tsoa spec-and-routes",
    "build": "tsoa spec-and-routes && tsc",
    "docker": "docker-compose -f docker-compose.yml -f docker-compose.production.yml up -d",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.development.yml up -d"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.14",
    "@types/express": "^4.17.11",
    "@types/mongoose": "^5.10.5",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vjpr/babel-plugin-parameter-decorator": "^1.0.15",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-loader": "^8.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.4",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.8.0",
    "validator": "^13.5.2",
    "winston": "^3.3.3"
  }
}
